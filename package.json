{
  "name": "alpino",
  "version": "1.0.0",
  "description": "Alpino API - node express mongodb",
  "main": "./dist/server.js",
  "scripts": {
    "test": "ts-node ./lib/server.ts",
    "devtsc": "tsc && nodemon ./dist/server.js",
    "dev": "ts-node-dev --respawn --ignore-watch node_modules --no-notify --transpile-only ./src/server.ts",
    "debug": "ts-node-dev --inspect --respawn --ignore-watch node_modules --no-notify --transpile-only ./src/server.ts",
    "prod": "tsc && nodemon ./dist/server.js",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "node",
    "mongodb",
    "express"
  ],
  "author": "Carlos Matias",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.0",
    "mongoose": "^5.13.15",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  }
}
